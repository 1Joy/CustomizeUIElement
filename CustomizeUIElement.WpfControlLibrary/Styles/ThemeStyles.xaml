<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <FontFamily x:Key="IconFontFamily">pack://application:,,,/CustomizeUIElement.WpfControlLibrary;component/Assets/Fonts/#iconfont</FontFamily>
    <!--前景色-->
    <SolidColorBrush x:Key="CustomizeForeground" Color="#fff"/>
    <SolidColorBrush x:Key="CustomizeForeground1" Color="#000"/>

    
    <SolidColorBrush x:Key="PrimaryColor" Color="#2775b6"/>
    <SolidColorBrush x:Key="DefaultColor" Color="#fff"/>
    <SolidColorBrush x:Key="HoverColor" Color="#5698c3"/>
    <SolidColorBrush x:Key="ForeBgColor" Color="#cdd1d3"/>
    <SolidColorBrush x:Key="BorderColor" Color="#5e616d"/>
    <SolidColorBrush x:Key="DisableColor" Color="Gray"/>

</ResourceDictionary>