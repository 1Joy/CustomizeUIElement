<Window x:Class="CustomizeUIElement.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:attachProp="clr-namespace:CustomizeUIElement.WpfControlLibrary.AttachPropertys;assembly=CustomizeUIElement.WpfControlLibrary"
        xmlns:local="clr-namespace:CustomizeUIElement.Demo"
        mc:Ignorable="d"
        Title="MainWindow" Width="800" Height="700">
    <Grid>
        <TabControl>
            <TabItem Header="button">
                <local:UCButtonView/>
            </TabItem>
            <TabItem Header="TextBox">
                <local:UCTextBoxView/>
            </TabItem>
        </TabControl>
        
    </Grid>
</Window>
